    "express": "^4.14.0",
    "getconfig": "^3.0.0",
    "lodash": "^4.17.4",
    "morgan": "^1.7.0"
  },
  "jshintConfig": {
    "asi": false,
    "browser": true,
    "curly": false,
    "expr": true,
    "indent": 4,
    "loopfunc": true,
    "node": true,
    "trailing": true,
    "undef": true,
    "white": true
  },
  "devDependencies": {
    "ampersand-app": "^2.0.0",
    "ampersand-form-view": "^7.0.0",

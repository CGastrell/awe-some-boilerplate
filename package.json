    "clientconfig": "^1.1.1",
    "css-loader": "^0.26.1",
    "domready": "^1.0.8",
    "dropzone": "^4.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "favicon-setter": "^0.1.1",
    "file-loader": "^0.9.0",
    "font-awesome": "^4.7.0",

{
  "name": "interactar-spa",
  "version": "0.0.1",
  "author": "Interactar S.R.L",
  "main": "server-express.js",
  "scripts": {
    "local": "NODE_ENV='localdev' ./node_modules/.bin/nodemon -e js,hbs,css ./server-express",
    "docker": "./node_modules/.bin/nodemon -e js,hbs,css ./server-express",
    "staging": "NODE_ENV='staging' node ./server-express",

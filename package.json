    "start": "node server-express",
    "build": "rimraf dist && cross-env NODE_ENV=production webpack --config ./webpack.production.config.js --progress --profile --colors",
    "compile": "NODE_ENV=production ./node_modules/webpack/bin/webpack.js --config ./webpack.production.config.js --progress --profile --colors",
    "eslint": "eslint .",
    "jscs": "jscs ."
  },
  "dependencies": {
